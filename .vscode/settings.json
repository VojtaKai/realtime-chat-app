{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact"
      ],
    // Runs Prettier, then ESLint
    "editor.codeActionsOnSave": {
        "source.formatDocument": true,
        "source.fixAll.eslint": true
    }
}